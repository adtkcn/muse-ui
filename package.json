{
  "name": "muse-ui",
  "version": "3.0.2",
  "description": "material design ui for vue2",
  "author": "xh <xh@adtk.cn>",
  "repository": "https://github.com/adtkcn/muse-ui.git",
  "main": "dist/muse-ui.common.js",
  "module": "dist/muse-ui.esm.js",
  "unpkg": "dist/muse-ui.js",
  "jsdelivr": "dist/muse-ui.js",
  "typings": "types/index.d.ts",
  "files": [
    "dist",
    "lib",
    "es5",
    "src",
    "types"
  ],
  "scripts": {
    "build": "rimraf dist && rollup --config rollup.config.js",
    "build:all": "npm run build && npm run build:lib && npm run build:es5",
    "build:lib": "rimraf lib && cross-env NODE_ENV=normal babel src -d lib --ignore *.spec.js --copy-files",
    "build:es5": "rimraf es5 && cross-env NODE_ENV=es5 babel src -d es5 --ignore *.spec.js --copy-files",
    "lint": "cross-env NODE_ENV=normal eslint --ext .js,.vue src",
    "test": "cross-env NODE_ENV=test jest -i",
    "test:coverage": "cross-env NODE_ENV=test jest -i --coverage",
    "prepush": "yarn run lint"
  },
  "peerDependencies": {
    "vue": "^2.6.10"
  },
  "devDependencies": {
    "autoprefixer": "^9.6.1",
    "avoriaz": "^6.3.0",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-eslint": "^10.0.3",
    "babel-helper-vue-jsx-merge-props": "^2.0.3",
    "babel-jest": "^24.9.0",
    "babel-plugin-syntax-jsx": "^6.18.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-plugin-transform-vue-jsx": "^3.7.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-es2015-rollup": "^3.0.0",
    "babel-preset-stage-2": "^6.24.1",
    "cross-env": "^5.2.1",
    "cssnano": "^4.1.10",
    "eslint": "^6.3.0",
    "eslint-config-vue": "^2.0.2",
    "eslint-plugin-vue": "^5.2.3",
    "husky": "^3.0.5",
    "jest": "^24.9.0",
    "jest-cli": "^24.9.0",
    "jest-css-modules": "^2.1.0",
    "jest-serializer-html": "^7.0.0",
    "less": "^3.10.3",
    "parcel-bundler": "^1.12.3",
    "parcel-plugin-vue": "^1.5.0",
    "postcss-import": "^12.0.1",
    "postcss-modules": "^1.4.1",
    "postcss-url": "^8.0.0",
    "rimraf": "^3.0.0",
    "rollup": "^1.20.3",
    "rollup-plugin-babel": "^3.0.7",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-postcss": "^2.0.3",
    "rollup-plugin-replace": "^2.2.0",
    "rollup-plugin-uglify": "^6.0.3",
    "typeface-roboto": "^0.0.75",
    "vue": "^2.6.10",
    "vue-jest": "^3.0.4",
    "vue-router": "^3.1.3",
    "vue-template-compiler": "^2.6.10"
  },
  "dependencies": {
    "body-scroll-lock": "^2.6.4",
    "dayjs": "^1.8.16",
    "keycode": "^2.2.0",
    "normalize-wheel": "^1.0.1",
    "resize-observer-polyfill": "^1.5.1"
  },
  "browserslist": [
    "> 1%",
    "last 5 versions",
    "ios >= 7",
    "android > 4.4",
    "not ie < 10"
  ]
}